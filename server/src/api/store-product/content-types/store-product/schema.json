{
  "kind": "collectionType",
  "collectionName": "strore_products",
  "info": {
    "singularName": "store-product",
    "pluralName": "strore-products",
    "displayName": "Produkty"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product-category.product-category"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "string"
    },
    "specifications": {
      "type": "component",
      "component": "elements.specification",
      "repeatable": true
    },
    "galleryImages": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
