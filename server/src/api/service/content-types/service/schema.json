{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Oferta"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text",
      "maxLength": 300
    },
    "icon": {
      "type": "component",
      "component": "elements.icon",
      "repeatable": false
    },
    "slug": {
      "type": "component",
      "component": "elements.link",
      "repeatable": false
    },
    "longDescription": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project"
    }
  }
}
